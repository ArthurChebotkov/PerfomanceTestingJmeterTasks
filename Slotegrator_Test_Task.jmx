              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">import org.apache.jmeter.gui.GuiPackage;
import org.apache.jmeter.util.JMeterUtils;

// Get the path of the JMeter test plan file
String testPlanPath = GuiPackage.getInstance().getTestPlanFile();

// Extract the directory path from the test plan file path
String testPlanDirectory = new File(testPlanPath).getParent();

String pathToEmails = vars.get(&quot;PATHEMAILS&quot;);

// Set the relative path by appending the desired file or directory name
String relativePath = testPlanDirectory + pathToEmails;


String email = vars.get(&quot;EMAIL&quot;);
// Assuming &quot;username&quot; is the JMeter variable name where you store the username.
// String filePath = relativePath +&quot;/email_addresses.csv&quot;; // Path to the CSV file.
FileOutputStream fileOutputStream = new FileOutputStream(relativePath, true);
PrintStream printStream = new PrintStream(fileOutputStream);

printStream.println(email);

printStream.close();
fileOutputStream.close();


</stringProp>
