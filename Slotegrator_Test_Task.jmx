// Get the path of the JMeter test plan file
String testPlanPath = GuiPackage.getInstance().getTestPlanFile();

// Extract the directory path from the test plan file path
String testPlanDirectory = new File(testPlanPath).getParent();

String pathToEmails = vars.get(&quot;PATHEMAILS&quot;);

// Set the relative path by appending the desired file or directory name
String relativePath = testPlanDirectory + pathToEmails;


// Specify the path to the CSV file
//String filePath = &quot;/Users/arthurchebotkov/Desktop/TaskForChezh/email_addresses.csv&quot;;

try {
    // Create a new FileWriter with append set to false
    FileWriter fileWriter = new FileWriter(new File(relativePath), false);

    // Write an empty string to the file
    fileWriter.write(&quot;&quot;);

    // Close the FileWriter
    fileWriter.close();

    // Log a message indicating successful deletion
    log.info(&quot;All data deleted from the CSV file: &quot; + filePath);
} catch (IOException e) {
    // Log an error message if an exception occurs
    log.error(&quot;Error occurred while deleting data from the CSV file: &quot; + filePath, e);
}
